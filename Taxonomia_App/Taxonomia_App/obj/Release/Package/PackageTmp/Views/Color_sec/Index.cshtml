@model IEnumerable<Taxonomia_App.Models.Color_sec>

@{
    ViewBag.Title = "Index";
}

<a class="navbar-brand" @Html.ActionLink("Taxonomia App", "Index", "SKUs") </a>
<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
</button>

<h2>Color Secundario</h2>

<p>
    @Html.ActionLink("Agregar", "Create")
</p>
<table class="table">

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Color_sec1)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.Id })
            </td>
        </tr>
    }

</table>
